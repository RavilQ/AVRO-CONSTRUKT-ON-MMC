@using AVRO_CONSTRUKTİON_MMC.Areas.Admin.ViewModels.SettingVMs

@{
    ViewData["Title"] = "Edit";
}
@model SettingPostVM

<div class="p-3 w-100 ">

    <form method="post"  id="form">
    <label class="form-label" asp-for="Value">@ViewBag.Key</label>
    <input asp-for="Value" class="form-control"/>
    <span asp-validation-for="Value" class="small text-danger"></span>
    </form>
    <button type="submit" form="form" class="btn btn-success mt-3">Yadda Saxla</button>

</div>

