@using AVRO_CONSTRUKTİON_MMC.Areas.Admin.ViewModels.JobVMs
@{
    ViewData["Title"] = "Create";
}
@model JobPostVM

<div class="p-3">

    <form asp-action="Create" asp-controller="Job" method="post"class="col-sm-6 ">

        <div class="my-3">
            <label asp-for="Name" class="form-label">Vəzifə Adı</label>
            <input asp-for="Name" class="form-control col-12" />
            <span asp-validation-for="Name" class="small text-danger"></span>
        </div>

        <button type="submit" class="btn btn-success"> Yarat </button>

    </form>
</div>

