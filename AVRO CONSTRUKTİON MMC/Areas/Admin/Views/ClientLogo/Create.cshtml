@using AVRO_CONSTRUKTİON_MMC.Areas.Admin.ViewModels.ClientLogoVMs

@{
    ViewData["Title"] = "Create";
}
@model ClientLogoPostVM
<div class="w-100 p-3">
    <form method="post" asp-action="Create" asp-controller="ClientLogo" enctype="multipart/form-data">
        
        <div class="my-3">
            <label class="form-label">Logo</label>
            <input asp-for="ImageFile" class="form-control"/>
            <span asp-validation-for="ImageFile" class="small text-danger"></span>
        </div>

        <button type="submit" class="btn btn-success">Əlavə et</button>
    </form>
</div>
